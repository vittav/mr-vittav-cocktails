<div class="container">
  <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :img_url %>
    <div class="d-flex justify-content-between">
      <%= f.submit "Create Cocktail", class: "btn btn-primary" %>
      <%= link_to "Home", cocktails_path, class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
